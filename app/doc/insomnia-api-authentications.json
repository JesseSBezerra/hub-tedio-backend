      "_id": "req_create_api_auth_oauth2",
      "parentId": "wrk_api_auth",
      "url": "{{ _.base_url }}/api/register/authentication",
      "name": "Create API Auth - OAuth2",
      "description": "Exemplo de autenticação OAuth2",
      "method": "POST",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"nome\": \"OAuth2 Google API\",\n  \"descricao\": \"Autenticação OAuth2 para Google APIs\",\n  \"ownerId\": 1,\n  \"url\": \"https://oauth2.googleapis.com/token\",\n  \"authenticationType\": \"OAUTH2\",\n  \"contentType\": \"APPLICATION_FORM_URLENCODED\",\n  \"requestBody\": {\n    \"grant_type\": \"client_credentials\",\n    \"client_id\": \"seu_client_id\",\n    \"client_secret\": \"seu_client_secret\",\n    \"scope\": \"https://www.googleapis.com/auth/drive\"\n  },\n  \"headers\": {\n    \"Accept\": \"application/json\"\n  }\n}"
      },
      "headers": [
        {"name": "Content-Type", "value": "application/json"},
        {"name": "Authorization", "value": "Bearer {{ _.jwt_token }}"}
      ],
      "_type": "request"
    },
    {
